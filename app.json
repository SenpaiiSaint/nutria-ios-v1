{
  "expo": {
    "name": "nutria-ios",
    "slug": "nutria-ios",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "nutriaios",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "assetBundlePatterns": [
        "/**/*",
        "/**/*"
      ],
      "bundleIdentifier": "com.mrmiyagi.nutriaios",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "android.permission.CAMERA"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-asset",
        {
          "assets": [
            "./assets/images/home-bg.jpg"
          ]
        }
      ],
      "expo-barcode-scanner"
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "73300302-16cb-43f9-9c69-1401a2bc73c3"
      }
    }
  }
}
